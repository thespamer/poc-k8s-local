{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "paths": {
      "@lib/emojis": [
        "libs/emojis/src"
      ],
      "@lib/emojis/*": [
        "libs/emojis/src/*"
      ],
      "@lib/emoji-svc-sdk": [
        "libs/emoji-svc-sdk/src"
      ],
      "@lib/emoji-svc-sdk/*": [
        "libs/emoji-svc-sdk/src/*"
      ],
      "@lib/voting-svc-sdk": [
        "libs/voting-svc-sdk/src"
      ],
      "@lib/voting-svc-sdk/*": [
        "libs/voting-svc-sdk/src/*"
      ]
    }
  }
}